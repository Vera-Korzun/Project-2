<div class="productInfo__wrapper">
  <div class="productInfo__images">
    <div class="productInfo__imgMax-wrapper">
      <img class="productInfo__imgMax" src="" alt="{{title}}" />
    </div>

    <ul class="list productInfo__list-imgMin">
      {{#each imageUrls}}
      <li class="productInfo__item-imgMin" data-url="{{this}}">
        <img class="productInfo__imgMin" src="{{this}}" alt="{{title}}" />
      </li>

      {{/each}}
    </ul>
  </div>


  <div class="productInfo__info">
    <h2 class="productInfo__title">{{title}}</h2>
    <p class="productInfo__code">Код товару | {{_id}}</p>


    <div class=" siema  productInfo__imgSlider">
      {{#each imageUrls}}
      <div class="productInfo__imgSlider-img">
        <img src="{{this}}" alt="{{title}}" />
      </div>
      {{/each}}
    </div>

    <div class="dots"></div>

    <p class="productInfo__price">{{price}} <span> &#8364;</span></p>
    <button class="productInfo__button" type="button" data-id="{{_id}}" data-category="{{category}}">
      Информация о <br>продавце
    </button>
    <div class="dealerInfo">
      <p><span class="productInfo__desctiption">Телефон: </span><span class="dealerTel productInfo__desctiption-text">+38**********</span></p>
    </div>
